  <PropertyGroup>
    <AderantBuildTasksVersion>1.0.282.0</AderantBuildTasksVersion>
    <AderantBuildTasksPath Condition="('$(AderantBuildTasksPath)'== '')">$(MSBuildExtensionsPath)\Aderant</AderantBuildTasksPath>
    <AderantBuildTasksV100Lib-1_0_0_0>$(AderantBuildTasksPath)\1.0.0.0\ADERANT.Build.Tasks.dll</AderantBuildTasksV100Lib-1_0_0_0>
    <AderantBuildClientTasksV100Lib-1_0_0_0>$(AderantBuildTasksPath)\1.0.0.0\ADERANT.Build.Client.Tasks.dll</AderantBuildClientTasksV100Lib-1_0_0_0>
    <AderantTargetsPath-1_0_0_0>$(AderantBuildTasksPath)\1.0.0.0</AderantTargetsPath-1_0_0_0>
    <!-- using a convention like this allows us to reference a single task with a specific version -->
    <MSBuildCommunityTasksLib-1_2_0_306>$(AderantBuildTasksPath)\MSBuildCommunityTasks\1.2.0.306\MSBuild.Community.Tasks.dll</MSBuildCommunityTasksLib-1_2_0_306>
    <SDCTasksLib-2_1_3041_0>$(AderantBuildTasksPath)\SDC\2.1.3041.0\Microsoft.Sdc.Tasks.dll</SDCTasksLib-2_1_3041_0>
    <XmlLabNxsltTasksLib-3_0_0_2238>$(AderantBuildTasksPath)\XmlLabNxsltTasks\3.0.0.2238\XmlLab.NxsltTasks.MSBuild.dll</XmlLabNxsltTasksLib-3_0_0_2238>
  </PropertyGroup>

  <!-- 
        All Aderant Build Tasks should be consolidated into a single assembly, unless development
        needs to own parts.  For now they are consolidated in a common targets (this file).
    -->
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.BuildConfig"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.CompareBuildNumber"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.DeploymentEngine"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.DumpFile"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.ExecCommand"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.EvalExpression"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.FrameworkDatabaseVersion"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.IncrementBuildNumber"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.MsiVersion"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.ReplaceStringInTextFile"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.ServiceUtils"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.SystemUtils"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.TFS.BuildDefinition"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.TFS.VersionControl"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.TouchFiles"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.Unzip"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.ValidateNetwork"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Tasks.v100.Zip"
        />

  <!-- 
        Aderant Build Client Tasks (Client facing used by Customization)
    -->
  <UsingTask
      AssemblyFile="$(AderantBuildClientTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Client.Tasks.v100.EnvironmentVariable"
        />
  <UsingTask
      AssemblyFile="$(AderantBuildClientTasksV100Lib-1_0_0_0)"
      TaskName="ADERANT.Build.Client.Tasks.v100.Transform"
        />
  <!-- 
        Referencing ThirdParty Build Tasks in this way identifies exposure to dependencies 
        and should prevent collisions. 
    -->

  <!-- Have to do this because MSBuild gets confused with Microsoft.Sdc.Tasks.Folder.Copy -->
  <!--<UsingTask TaskName="Copy" AssemblyName="Microsoft.Build.Tasks, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>-->
  <UsingTask
      AssemblyName="Microsoft.Build.Tasks.v3.5, Version=3.5.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"
      TaskName="Microsoft.Build.Tasks.Copy"
        />

  <!-- Specific MSBuildCommunityTasks refereced here. -->
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.Attrib"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.FileUpdate"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.Move"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.RegexMatch"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.RegexReplace"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.RegistryRead"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.RegistryWrite"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.TemplateFile"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.Unzip"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.Xml.XmlQuery"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.XmlRead"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.XmlUpdate"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.Xslt"
        />
  <UsingTask
      AssemblyFile="$(MSBuildCommunityTasksLib-1_2_0_306)"
      TaskName="MSBuild.Community.Tasks.Zip"
        />

  <!-- Specific SDC Tasks referenced here.  There is a problem importing the entire task lib -->
  <UsingTask
      AssemblyFile="$(SDCTasksLib-2_1_3041_0)"
      TaskName="Microsoft.Sdc.Tasks.File.GetFiles"
        />
  <UsingTask
      AssemblyFile="$(SDCTasksLib-2_1_3041_0)"
      TaskName="Microsoft.Sdc.Tasks.File.Replace"
        />
  <UsingTask
      AssemblyFile="$(SDCTasksLib-2_1_3041_0)"
      TaskName="Microsoft.Sdc.Tasks.Folder.Copy"
        />
  <UsingTask
      AssemblyFile="$(SDCTasksLib-2_1_3041_0)"
      TaskName="Microsoft.Sdc.Tasks.GetEnvironmentVariable"
        />
  <UsingTask
      AssemblyFile="$(SDCTasksLib-2_1_3041_0)"
      TaskName="Microsoft.Sdc.Tasks.SetEnvironmentVariable"
        />
  <UsingTask
      AssemblyFile="$(SDCTasksLib-2_1_3041_0)"
      TaskName="Microsoft.Sdc.Tasks.ServiceProcess.ControlService"
        />
  <UsingTask
      AssemblyFile="$(SDCTasksLib-2_1_3041_0)"
      TaskName="Microsoft.Sdc.Tasks.ServiceProcess.Exists"
        />
  <UsingTask
      AssemblyFile="$(SDCTasksLib-2_1_3041_0)"
      TaskName="Microsoft.Sdc.Tasks.Sleep"
        />
  <UsingTask
      AssemblyFile="$(SDCTasksLib-2_1_3041_0)"
      TaskName="Microsoft.Sdc.Tasks.StringToItemList"
        />


  <UsingTask
      AssemblyFile="$(XmlLabNxsltTasksLib-3_0_0_2238)"
      TaskName="XmlLab.NxsltTasks.MSBuild.Nxslt"
        />
  
  <UsingTask
      AssemblyFile="C:\Program Files (x86)\MSBuild\NDepend\NDepend.Build.MSBuild.dll"
      TaskName="NDependTask"
        />


  <Import Project="RM.TransformTemplates.Targets"/>


  <!-- Allow Developers to have custom targets loaded, This is after RM's so over-rides could occur -->
  <Import
      Condition="Exists('$(AderantBuildTasksPath)\Developer.Custom.Targets')"
      Project="$(AderantBuildTasksPath)\Developer.Custom.Targets"
        />

</Project>
