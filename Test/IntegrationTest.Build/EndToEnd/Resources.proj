<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="14.0">

  <PropertyGroup>
    <_DependencyAnalysisIncludePattern>
      $(MSBuildProjectDirectory)\EndToEnd\Resources\[0]\**\*.cs;
      $(MSBuildProjectDirectory)\EndToEnd\Resources\[0]\**\*.csproj;
      $(MSBuildProjectDirectory)\EndToEnd\Resources\[0]\**\*.sln;
      $(MSBuildProjectDirectory)\EndToEnd\Resources\[0]\**\*.tt;
      $(MSBuildProjectDirectory)\EndToEnd\Resources\[0]\**\*.proj;
      $(MSBuildProjectDirectory)\EndToEnd\Resources\[0]\**\*.rsp;
      $(MSBuildProjectDirectory)\EndToEnd\Resources\[0]\**\*.xml;
      $(MSBuildProjectDirectory)\EndToEnd\Resources\[0]\**\*.ps1;
      $(MSBuildProjectDirectory)\EndToEnd\Resources\[0]\**\*.targets;
    </_DependencyAnalysisIncludePattern>
    <_DependencyAnalysisExcludePattern>
      **\obj\**;
      **\bin\**;
      TestDeployment\**
    </_DependencyAnalysisExcludePattern>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="$(_DependencyAnalysisIncludePattern)" Exclude="$(_DependencyAnalysisExcludePattern)">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Visible>true</Visible>
    </Content>
  </ItemGroup>

</Project>

