<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="14.0">

  <Target Name="GenerateFactory">

    <PropertyGroup>
      <FactoryResourceGenerator>$(ExpertSourceDirectory)\FactoryResourceGenerator.exe</FactoryResourceGenerator>
    </PropertyGroup>

    <Exec ContinueOnError="false"
      IgnoreExitCode="false"
      Command="$(FactoryResourceGenerator) /v:+ /f:&quot;$(ExpertSourceDirectory)&quot; /of:&quot;$(ExpertSourceDirectory)\Factory.bin&quot; /sp:&quot;Aderant*.dll`,*.exe&quot;" />

  </Target>

</Project>
