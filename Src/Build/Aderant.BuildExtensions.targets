<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!--
    This file is intended to hold all targets that a developer can override or implement to customize the build.
    The targets in this file will be invoked when recursing into a directory.
  -->

  <!--Default implementation-->
  <Target Name="GetStagingDirectoryWhitelist" Returns="@(StagingDirectoryWhitelist)">    
    <ItemGroup>
      <StagingDirectoryWhitelist Include="UnitTest.Framework.Helpers.dll" />
    </ItemGroup>
    
    <Message Text="Default @(StagingDirectoryWhitelist)" />
  </Target>

  <Import Project="$(ExtensibilityFile)" Condition="'$(ExtensibilityFile)' != ''" />

</Project>
